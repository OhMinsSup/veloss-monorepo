{
  "name": "@veloss/openapi-ofetch",
  "description": "openapi-fetch extension library (ofetch style)",
  "author": {
    "name": "OhMinsSup",
    "email": "mins5190@gmail.com"
  },
  "version": "0.0.6",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OhMinsSup/veloss-monorepo",
    "directory": "packages/openapi-ofetch"
  },
  "bugs": {
    "url": "https://github.com/OhMinsSup/veloss-monorepo/issues"
  },
  "keywords": [
    "openapi",
    "openapi-fetch",
    "openapi-client",
    "javascript",
    "typescript",
    "rest",
    "api",
    "oapi_3",
    "oapi_3_1",
    "fetch"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "coverage": "vitest run --coverage",
    "dev": "tsup --watch",
    "format": "biome format . --write",
    "lint": "biome check .",
    "test": "vitest run",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "docs": "typedoc src/index.ts --out docs"
  },
  "dependencies": {
    "@veloss/error": "workspace:*"
  },
  "peerDependencies": {
    "openapi-fetch": "^0.13.4",
    "openapi-typescript-helpers": "^0.0.15"
  },
  "devDependencies": {
    "h3": "^1.14.0",
    "listhen": "^1.9.0",
    "ufo": "^1.5.4",
    "vite": "catalog:vitejs",
    "vite-tsconfig-paths": "catalog:vitejs",
    "jsdom": "catalog:vitejs",
    "vitest": "catalog:vitejs",
    "@veloss/tsconfig": "workspace:*",
    "tsup": "catalog:",
    "typedoc": "catalog:",
    "typescript": "catalog:"
  }
}
