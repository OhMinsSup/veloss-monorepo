{
  "name": "@veloss/error",
  "description": "Error handling utilities",
  "author": {
    "name": "OhMinsSup",
    "email": "mins5190@gmail.com"
  },
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./http": {
      "import": {
        "types": "./dist/http.d.ts",
        "default": "./dist/http.js"
      },
      "require": {
        "types": "./dist/http.d.cts",
        "default": "./dist/http.cjs"
      }
    },
    "./auth": {
      "import": {
        "types": "./dist/auth.d.ts",
        "default": "./dist/auth.js"
      },
      "require": {
        "types": "./dist/auth.d.cts",
        "default": "./dist/auth.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "tsup --watch",
    "format": "biome format . --write",
    "lint": "biome check .",
    "test": "vitest",
    "test:run": "vitest run",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "devDependencies": {
    "vite": "catalog:vitejs",
    "vite-tsconfig-paths": "catalog:vitejs",
    "vitest": "catalog:vitejs",
    "@veloss/tsconfig": "workspace:*",
    "tsup": "catalog:",
    "typescript": "catalog:"
  }
}
